<!doctype html>
<html class="no-js" lang="en">
  <head profile="http://www.w3.org/2005/10/profile">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{{ title }} | Florian Schrofner</title>
    
    <link rel="icon" type="image/png" href="{{ base_url }}/favicon.png"/>
    <link rel="stylesheet" href="{{ base_url }}/styles/foundation/css/foundation.css" />
    <link rel="stylesheet" href="{{ base_url }}/styles/prism/prism.css"/>
    <link rel="stylesheet" href="{{ base_url }}/styles/font-awesome/css/font-awesome.min.css"/>
    <link rel="stylesheet" href="{{ base_url }}/styles/style.css" />
    
    <script src="{{ base_url }}/styles/foundation/js/modernizr.min.js"></script>
    
  </head>
  <div class="off-canvas-wrap">
    <div class="inner-wrap">
      <body>
        <section class="main-section">
          {{> header}}
          
          <section id="content">
            <div class="row">
              <div class="small-12 columns">
                {{> blog_posts}}
              </div>
            </div>
          </section>
          {{> footer}}
          
          <a class="exit-off-canvas"></a>
          <script src="{{ base_url }}/styles/foundation/js/jquery.min.js"></script>
          <script src="{{ base_url }}/styles/foundation/js/foundation/foundation.min.js"></script>
          <script src="{{ base_url }}/styles/foundation/js/foundation.min.js"></script>
          <script src="{{ base_url }}/styles/foundation/js/foundation/foundation.clearing.min.js"></script>
          <script src="{{ base_url }}/scripts/sticky_footer.min.js"></script>
          <script src="{{ base_url }}/styles/prism/prism.min.js"></script>
          <script src="{{ base_url }}/scripts/apply_foundation.min.js"></script>
        </section>
      </body>
    </div>
  </div>
</html>


